# Base16 Windows High Contrast Light
# Author: Fergus Collins (https://github.com/C-Fergus)

_gen_fzf_default_opts() {

local color00='#fcfcfc'
local color01='#e8e8e8'
local color02='#d4d4d4'
local color03='#c0c0c0'
local color04='#7e7e7e'
local color05='#545454'
local color06='#2a2a2a'
local color07='#000000'
local color08='#800000'
local color09='#fcfc54'
local color0A='#808000'
local color0B='#008000'
local color0C='#008080'
local color0D='#000080'
local color0E='#800080'
local color0F='#54fc54'

export FZF_DEFAULT_OPTS="$FZF_DEFAULT_OPTS"\
" --color=bg+:$color01,bg:$color00,spinner:$color0C,hl:$color0D"\
" --color=fg:$color04,header:$color0D,info:$color0A,pointer:$color0C"\
" --color=marker:$color0C,fg+:$color06,prompt:$color0A,hl+:$color0D"

}

_gen_fzf_default_opts
